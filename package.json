{
  "name": "Liga-A-start-template",
  "version": "1.0.0",
  "description": "Стартовый шаблон сборки Лиги А",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "9.7.x",
    "browser-sync": "2.26.x",
    "browserify": "16.5.0",
    "eslint": "6.8.0",
    "eslint-config-htmlacademy": "0.5.x",
    "gulp": "4.0.x",
    "gulp-concat": "^2.6.1",
    "gulp-csso": "^4.0.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "8.0.x",
    "gulp-posthtml": "^3.0.4",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "4.0.x",
    "gulp-sourcemaps": "2.6.x",
    "gulp-svgstore": "^7.0.1",
    "gulp-webp": "^4.0.1",
    "pixel-glass": "^1.0.13",
    "posthtml-include": "^1.4.0",
    "stylelint": "13.2.x",
